{
	"activationEvents": [
		"*"
	],
	"author": {
		"email": "matt@polymermallard.com",
		"name": "Matt Kenefick"
	},
	"bugs": {
		"url": "https://github.com/mattkenefick/vscode-icon-snippets/issues"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"capabilities": {
			"hoverProvider": "true"
		},
		"configuration": [
			{
				"properties": {
					"iconSnippets.iconColor": {
						"default": "#bababa",
						"description": "",
						"pattern": "^#([0-9a-fA-F]{3}){1,2}$",
						"type": "string"
					},
					"iconSnippets.iconSize": {
						"default": 100,
						"description": "",
						"minimum": 10,
						"type": "integer"
					},
					"iconSnippets.selector": {
						"default": [
							"html",
							"javascript",
							"javascriptreact",
							"ng-template",
							"typescript",
							"typescriptreact",
							"vue-html",
							"vue-pug",
							"vue"
						],
						"description": "Defines the documents where completion and hover are active",
						"items": {
							"type": "string"
						},
						"type": "array",
						"uniqueItems": true
					}
				},
				"title": "Insert SVG icons to the active document"
			}
		]
	},
	"dependencies": {
		"vsce": "^2.6.7"
	},
	"description": "Insert SVG icons with preview",
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"displayName": "Icon Snippets",
	"engines": {
		"vscode": "^1.57.0"
	},
	"galleryBanner": {
		"color": "#d9d9d9",
		"theme": "light"
	},
	"homepage": "https://github.com/mattkenefick/vscode-icon-snippets",
	"icon": "assets/logo.png",
	"license": "MIT",
	"main": "./dist/extension.js",
	"name": "icon-snippets",
	"publisher": "PolymerMallard",
	"repository": {
		"type": "git",
		"url": "https://github.com/mattkenefick/vscode-icon-snippets.git"
	},
	"scripts": {
		"compile": "webpack",
		"lint": "eslint src --ext ts",
		"package": "webpack --mode production --devtool hidden-source-map",
		"pretest": "yarn run test-compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"vscode:prepublish": "yarn run package",
		"watch": "webpack --watch"
	},
	"version": "0.2.3"
}
